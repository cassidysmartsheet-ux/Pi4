config/network.conf
config/urls.local.conf

# Local environment overrides
*.local
*.local.*
.env
.env.*

# Logs
logs/
*.log

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# IDE files
.idea/
.vscode/
*.swp
*.swo
*~

# Backup files
*.bak
*.backup
backups/

# SD card images
*.img
*.iso

# Temporary files
tmp/
temp/
